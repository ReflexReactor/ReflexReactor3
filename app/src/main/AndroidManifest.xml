<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amit.reflexreactor">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.GET_TASKS" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <permission
            android:name="com.example.gcm.permission.C2D_MESSAGE"
            android:protectionLevel="signature" />

        <uses-permission android:name="com.example.gcm.permission.C2D_MESSAGE" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Single_player"
            android:label="@string/title_activity_single_player"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Multi_player"
            android:label="@string/title_activity_multi_player"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Score_board"
            android:label="@string/title_activity_score_board"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Invites"
            android:label="@string/title_activity_invites"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Online"
            android:label="@string/title_activity_online"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SendingActivity"
            android:label="@string/title_activity_online"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Offline"
            android:label="@string/title_activity_offline"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Create_game"
            android:label="@string/title_activity_create_game"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Join_game"
            android:label="@string/title_activity_join_game"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Category"
            android:label="@string/title_activity_category"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Questions"
            android:label="@string/title_activity_questions"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Home2"
            android:label="ReflexReactor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Register"
            android:label="@string/title_activity_register"
            android:theme="@style/AppTheme.NoActionBar" />

        <service
            android:name=".PushNotificationService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>

        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />

                <category android:name="com.example.shweta.gcmsampleapp" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".OnlineListActivity"
            android:label="@string/title_activity_online_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RequestActivity"
            android:label="@string/title_activity_request"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Accepted"
            android:label="@string/title_activity_accepted"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".scoreboard2"
            android:label="@string/title_activity_scoreboard2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ResultActivity"
            android:label="@string/title_activity_result"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ResultActivity2"
            android:label="@string/title_activity_result2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".InternetActivity"
            android:label="@string/title_activity_internet"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AcceptedChallenge"
            android:label="@string/title_activity_accepted_challenge"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".scoreboard3"></activity>
    </application>

</manifest>
